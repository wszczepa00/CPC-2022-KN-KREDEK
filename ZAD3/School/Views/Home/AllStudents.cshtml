@model IEnumerable<School.Models.Student>

@{
    ViewData["Title"] = "Courses";
}

<h1>Studenci: </h1>

<table class="table">
    <thead>
        <tr>
            <th>
                StudentId
            </th>
            <th>
                Imię
            </th>
            <th>
                Nazwisko
            </th>
            <th>
                Kierunek
            </th>
            <th>
                Numer Indeksu
            </th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.StudentId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Field)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Index)
                </td>
            </tr>
        }
    </tbody>
</table>
